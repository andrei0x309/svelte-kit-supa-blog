{
	"name": "svelte-kit-supa-blog",
	"version": "0.0.2",
	"scripts": {
		"dev": "node --experimental-network-imports ./node_modules/vite/bin/vite dev",
		"build": "node --experimental-network-imports ./node_modules/vite/bin/vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.37.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/kit": "^1.24.1",
		"@types/cookie": "^0.5.2",
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"@vite-pwa/sveltekit": "^0.2.7",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.29",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"sass": "^1.66.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.5.1",
		"svelte-lightbox": "^1.1.2",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-compatible-readable-stream": "^3.6.1",
		"vite-plugin-node-polyfills": "^0.12.0",
		"vite-plugin-pwa": "^0.16.5",
		"vitest": "^0.34.4",
		"workbox-build": "^7.0.0",
		"workbox-window": "^7.0.0",
		"yarn-upgrade-all": "^0.7.2"
	},
	"type": "module",
	"license": "MIT",
	"dependencies": {
		"@builder.io/partytown": "^0.8.1",
		"@supabase/supabase-js": "^2.33.1",
		"highlight.js": "^11.8.0",
		"jodit": "^4.0.0-beta.61",
		"truncate-html": "^1.0.4"
	},
	"browser": {
		"stream": false
	}
}
