{
	"name": "svelte-kit-supa-blog",
	"version": "0.0.2",
	"scripts": {
		"dev": "bun --experimental-network-imports ./node_modules/vite/bin/vite dev",
		"build": "bun --experimental-network-imports ./node_modules/vite/bin/vite build",
		"cron": "yarn tsx ./src/cli/cron.ts",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"outdated": "bunx ncu --packageManager bun --root --format group -i",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-netlify": "^4.3.1",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/cookie": "^0.6.0",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@vite-pwa/sveltekit": "^0.6.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.8.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"npm-check-updates": "^16.14.20",
		"postcss": "^8.4.40",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"sass": "^1.77.8",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"svelte-lightbox": "^1.1.3",
		"svelte-preprocess": "^6.0.2",
		"tailwindcss": "^3.4.7",
		"tslib": "^2.6.3",
		"tsx": "^4.16.2",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"vite-compatible-readable-stream": "^3.6.1",
		"vite-plugin-node-polyfills": "^0.22.0",
		"vite-plugin-pwa": "^0.20.1",
		"vitest": "^2.0.4",
		"workbox-build": "^7.1.1",
		"workbox-window": "^7.1.0",
		"yarn-upgrade-all": "^0.7.2"
	},
	"type": "module",
	"license": "MIT",
	"dependencies": {
		"@builder.io/partytown": "^0.10.2",
		"@supabase/supabase-js": "^2.45.0",
		"eslint-plugin-svelte": "^2.43.0",
		"highlight.js": "^11.10.0",
		"jodit": "^3.24.7",
		"truncate-html": "^1.1.1"
	},
	"browser": {
		"stream": false
	},
	"packageManager": "yarn@4.0.2"
}
